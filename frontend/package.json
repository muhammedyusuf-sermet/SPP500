    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "jest": "^24.1.0",
    "react-form": "^3.5.7",
    "tslint": "^5.12.1",
    "tslint-consistent-codestyle": "^1.15.0",
    "typescript": "^3.3.3",
    "webpack": "4.28.4"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
